{
  "name": "studujdopravu",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-watch": "DEBUG=studujdopravu:* nodemon -i src/ -i public/ -e js,json,pug,css ./bin/www",
    "build-watch": "parcel watch src/js/*.js src/styles/*.scss --out-dir public",
    "build": "parcel build src/js/*.js src/img/*.png src/img/svg/*.svg src/img/icon/*.png src/styles/*.scss --out-dir public",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-watch\"",
    "start": "npm run build && node ./bin/www"
  },
  "dependencies": {
    "@iconfu/svg-inject": "^1.2.3",
    "bcrypt": "^3.0.7",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "quill": "^1.3.7",
    "quill-blot-formatter": "^1.0.5",
    "quill-delta-to-html": "^0.11.0",
    "speakingurl": "^14.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-imagemin": "^4.0.0",
    "sass": "^1.24.4"
  }
}
